{
    "name": "shooglypeg/datavalidator",
    "description": "Validate incoming data using json schema",
    "keywords": [],
    "homepage": "https://github.com/pedros80/ShooglyPeg-DataValidator",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Somerville",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "league/flysystem": "^1.0",
        "opis/json-schema": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ShooglyPeg\\DataValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
